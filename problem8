#include <iostream>
using namespace std;

bool isStrictlyIncreasing(int n) {
    if (n < 10)
        return true;

    int lastDigit = n % 10;
    int secondLastDigit = (n / 10) % 10;

    if (secondLastDigit >= lastDigit)
        return false;

    return isStrictlyIncreasing(n / 10);
}

int main() {
    int n;
    cout << "Enter a number: ";
    cin >> n;

    if (isStrictlyIncreasing(n))
        cout << n << " has strictly increasing digits.";
    else
        cout << n << " does not have strictly increasing digits.";

    return 0;
}
